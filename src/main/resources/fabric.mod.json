    "main": [
      "dev.mariany.mobilityflux.MobilityFlux"
    ],
    "client": [
      "dev.mariany.mobilityflux.client.MobilityFluxClient"
    ],
    "fabric-datagen": [
      "dev.mariany.mobilityflux.MobilityFluxDataGenerator"
    ]
  },
  "mixins": [
    "mobilityflux.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": ">=1.21.6",
    "java": ">=21",
    "fabric-api": ">=0.128.2"
  }
}