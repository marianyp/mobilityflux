  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_page_url}",
    "sources": "${mod_git_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/mobilityflux/icon.png",
  "environment": "*",
